{
  "name": "phone-catalog-frontend",
  "version": "1.0.0",
  "description": "Frontend for phone-catalog app",
  "main": "index.js",
  "scripts": {
    "build": "if-env NODE_ENV=production && npm run build:prod || npm run build:dev",
    "build:dev": "webpack --config ./config/webpack/dev.js",
    "build:prod": "webpack --config ./config/webpack/prod.js",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "env-cmd development webpack-dev-server --config ./config/webpack/dev.js",
    "start:prod": "node server",
    "test": "jest -c ./config/test/jest.json --verbose",
    "test:watch": "jest -c ./config/test/jest.json --verbose --watchAll -i"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nasdan/phone-catalog.git"
  },
  "keywords": [
    "phone",
    "catalog",
    "frontend"
  ],
  "author": "nasdan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Nasdan/phone-catalog/issues"
  },
  "homepage": "https://github.com/Nasdan/phone-catalog#readme",
  "dependencies": {
    "@material-ui/core": "^3.1.0",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "connected-react-router": "^4.4.1",
    "emotion": "^9.2.9",
    "express": "^4.16.3",
    "history": "^4.7.2",
    "prop-types": "^15.6.2",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-promise-tracker": "^1.1.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-spinners": "^0.4.5",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-emotion": "^9.2.9",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "deep-freeze": "0.0.1",
    "env-cmd": "^8.0.2",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "if-env": "^1.0.4",
    "jest": "^23.6.0",
    "raf": "^3.4.0",
    "react-hot-loader": "^4.3.8",
    "redux-mock-store": "1.4.0",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-merge": "^4.1.4"
  }
}
